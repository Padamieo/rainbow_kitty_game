{
  "name": "flatulant_rainbow_kitty",
  "version": "0.0.1",
  "author": "Micha√©l Adam Lockwood <Michaeladamlockwood@gmail.com> (https://github.com/Padamieo)",
  "description": "re build of rainbow kitty in phaser 3",
  "license": "MIT",
  "keywords": [
    "webpack"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.development.js --mode development",
    "live": "webpack-dev-server --config config/webpack.live.js --mode production",
    "stage": "webpack --env=stage --progress --profile --colors --mode development",
    "build": "webpack --env=production --progress --profile --colors --mode production",
    "lint": "eslint ./src/**.js",
    "coverage": "jest --coverage --config=jest.config.js",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --watch",
    "size": "yarn build && size-limit",
    "size:why": "size-limit --why",
    "ssl": "openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout ssl/server.key -out ssl/server.crt"
  },
  "sideEffects": false,
  "size-limit": [
    {
      "path": "dist/app.*.js",
      "webpack": true
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-decorators": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@size-limit/preset-big-lib": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "core-js": "^3.6.5",
    "css-loader": "^0.28.5",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.19.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "raw-loader": "^1.0.0",
    "react-hot-loader": "^4.12.21",
    "size-limit": "^4.5.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^3.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "phaser": "^3.22.0",
    "preact": "^10.4.1"
  }
}
